{
  "name": "qms-question-parser",
  "version": "1.0.0",
  "description": "Question Management System Parser",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose --config jest.config.js",
    "test:watch": "jest --watch --config jest.config.js",
    "test:coverage": "jest --coverage --config jest.config.js",
    "test:ci": "jest --ci --coverage --config jest.config.js",
    "clasp:push": "clasp push",
    "clasp:pull": "clasp pull",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "author": "[Your Name]",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/runtime": "^7.24.0",
    "@google/clasp": "^2.4.2",
    "@types/google-apps-script": "^1.0.67",
    "@types/jest": "^29.5.14",
    "babel-jest": "^29.7.0",
    "core-js": "^3.36.0",
    "eslint": "^8.49.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "eslintConfig": {
    "extends": "google",
    "env": {
      "jest": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "max-len": [
        "error",
        {
          "code": 100
        }
      ],
      "require-jsdoc": 0
    }
  },
  "repository": {
    "type": "git",
    "url": "your-repository-url"
  },
  "keywords": [
    "google-apps-script",
    "sheets",
    "parser",
    "questions"
  ]
}
